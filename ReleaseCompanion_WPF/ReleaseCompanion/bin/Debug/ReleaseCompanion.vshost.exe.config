<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <add key="FoldersToCopyForBackup" value="bin,Content,DomainAssemblies,Lw.WebServices,Lw.WindowsServices,Views,ViewScripts,XSD_Template,Scripts" />
